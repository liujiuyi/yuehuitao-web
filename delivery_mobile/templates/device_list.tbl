 <div class="container">
  <div class="header">
   <img src="images/mlogo.png" style="margin: 10px 0px 0px 15px" />
  </div>
  <div class="device-list">
     {foreach from=$device_list item=device_data}
     <div class="device" onclick="box_list({$device_data.id})">
        {$device_data.device_name}
     </div>
     {/foreach}
  </div>
 <div class="footer">
